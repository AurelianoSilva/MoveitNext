         levelUp();\r\n        }\r\n\r\n        setCurrentExperience(fina)\r\n\r\n    }\r\n\r\n\r\n    return (\r\n        <ChallengesContext.Provider \r\n        value={{ \r\n            level, \r\n            currentExperience, \r\n            experienceToNextLevel,\r\n            challengesCompleted, \r\n            levelUp,\r\n            startNewChallenge,\r\n            activeChallenge,\r\n            resetChallenge,\r\n            completeChallenge,\r\n            \r\n         }}\r\n         >\r\n            {children}\r\n        </ChallengesContext.Provider>\r\n    )\r\n\r\n}"]},"metadata":{},"sourceType":"module"}